<UserControl x:Class="iChing.HexagramImagesPortugese"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:iChing"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300" Width="Auto" Height="Auto" VerticalContentAlignment="Top" HorizontalContentAlignment="Left">
    <UserControl.OpacityMask>
        <RadialGradientBrush GradientOrigin="0.5,0.5" Center="0.5,0.5">
            <GradientStop Color="White" Offset="0"/>
            <GradientStop Color="#19FFFFFF" Offset="0.998"/>
        </RadialGradientBrush>
    </UserControl.OpacityMask>
    <Image x:Name="image" Source="18.jpg" Stretch="Fill"/>
</UserControl>
