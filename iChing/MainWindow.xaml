<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:System="clr-namespace:System;assembly=mscorlib" x:Class="iChing.MainWindow"
        Title="i Ching" WindowStartupLocation="CenterScreen" ResizeMode="NoResize" Height="730" Width="700" HorizontalAlignment="Left" VerticalAlignment="Center" KeyUp="Window_KeyUp">
    <Window.Resources>
        <LinearGradientBrush x:Key="Yin" EndPoint="0.5,1" MappingMode="RelativeToBoundingBox" StartPoint="0.5,0">
            <LinearGradientBrush.RelativeTransform>
                <TransformGroup>
                    <ScaleTransform CenterY="0.5" CenterX="0.5"/>
                    <SkewTransform CenterY="0.5" CenterX="0.5"/>
                    <RotateTransform Angle="90" CenterY="0.5" CenterX="0.5"/>
                    <TranslateTransform/>
                </TransformGroup>
            </LinearGradientBrush.RelativeTransform>
            <GradientStop Color="White" Offset="0"/>
            <GradientStop Color="White" Offset="1"/>
            <GradientStop Color="White" Offset="0.333333"/>
            <GradientStop Color="#00000000" Offset="0.333333"/>
            <GradientStop Color="#00000000" Offset="0.66666"/>
            <GradientStop Color="White" Offset="0.66666"/>
        </LinearGradientBrush>
        <LinearGradientBrush x:Key="Yang" EndPoint="0.5,1" MappingMode="RelativeToBoundingBox" StartPoint="0.5,0">
            <LinearGradientBrush.RelativeTransform>
                <TransformGroup>
                    <ScaleTransform CenterY="0.5" CenterX="0.5"/>
                    <SkewTransform CenterY="0.5" CenterX="0.5"/>
                    <RotateTransform Angle="90" CenterY="0.5" CenterX="0.5"/>
                    <TranslateTransform/>
                </TransformGroup>
            </LinearGradientBrush.RelativeTransform>
            <GradientStop Color="White" Offset="0"/>
            <GradientStop Color="White" Offset="1"/>
        </LinearGradientBrush>
        <Thickness x:Key="SmallNamesMargin">0,0,-10,10</Thickness>
        <System:Double x:Key="SizeOfLines">25</System:Double>
    </Window.Resources>
    <Window.Background>
        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
            <GradientStop Color="Black" Offset="0"/>
            <GradientStop Color="White" Offset="1"/>
            <GradientStop Color="#FF313131" Offset="0.783"/>
        </LinearGradientBrush>
    </Window.Background>
    <Grid HorizontalAlignment="Left" Height="696" Margin="10,0,0,0" VerticalAlignment="Top" Width="680">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="30*"/>
            <ColumnDefinition Width="19*"/>
            <ColumnDefinition Width="19*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="70*"/>
            <RowDefinition Height="259*"/>
            <RowDefinition Height="201*"/>
            <RowDefinition Height="166*"/>
        </Grid.RowDefinitions>
        <TextBox x:Name="Comments" HorizontalAlignment="Left" Height="161" Margin="10,10,0,-5" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="660" Grid.Row="3" Grid.ColumnSpan="3"/>
        <ScrollViewer Margin="10" RenderTransformOrigin="0.5,0.5" Grid.Row="2">
            <StackPanel HorizontalAlignment="Left" VerticalAlignment="Top" Width="225">
                <TextBox x:Name="TextBox1" TextWrapping="Wrap" Text="Tb" PreviewMouseDown="TextBox1_PreviewMouseDown" BorderThickness="0" SelectionBrush="#003399FF" IsInactiveSelectionHighlightEnabled="True" IsReadOnlyCaretVisible="True" Focusable="False" AllowDrop="False" Margin="0,0,-55,0"/>
                <TextBox x:Name="TextBox2" TextWrapping="Wrap" Text="Tb" PreviewMouseDown="TextBox1_PreviewMouseDown" BorderThickness="0" SelectionBrush="#003399FF" IsInactiveSelectionHighlightEnabled="True" IsReadOnlyCaretVisible="True" Focusable="False" AllowDrop="False" Margin="0,0,-55,0"/>
                <TextBox x:Name="TextBox2_Copy1" TextWrapping="Wrap" Text="Tb" PreviewMouseDown="TextBox1_PreviewMouseDown" BorderThickness="0" SelectionBrush="#003399FF" IsInactiveSelectionHighlightEnabled="True" IsReadOnlyCaretVisible="True" Focusable="False" AllowDrop="False" Margin="0,0,-55,0"/>
                <TextBox x:Name="TextBox2_Copy2" TextWrapping="Wrap" Text="                                                                                                    " PreviewMouseDown="TextBox1_PreviewMouseDown" BorderThickness="0" SelectionBrush="#003399FF" IsInactiveSelectionHighlightEnabled="True" IsReadOnlyCaretVisible="True" Focusable="False" AllowDrop="False" Margin="0,0,-55,0" Height="163"/>
            </StackPanel>
        </ScrollViewer>
        <StackPanel Margin="10,10,0,10" Grid.Row="2" Grid.Column="1">
            <Rectangle x:Name="Yin" Margin="0,0,0,4" Fill="{DynamicResource Yin}" PreviewMouseUp="Yin_PreviewMouseUp" Height="{DynamicResource SizeOfLines}"/>
            <Rectangle x:Name="Line5" Height="{DynamicResource SizeOfLines}" Margin="0,0,0,4" Fill="{DynamicResource Yin}"/>
            <Rectangle x:Name="Line4" Height="{DynamicResource SizeOfLines}" Margin="0,0,0,4" Fill="{DynamicResource Yin}"/>
            <Rectangle x:Name="Yin_Copy2" Height="{DynamicResource SizeOfLines}" Margin="0,0,0,4" Fill="{DynamicResource Yin}"/>
            <Rectangle x:Name="Yin_Copy3" Height="{DynamicResource SizeOfLines}" Margin="0,0,0,4" Fill="{DynamicResource Yin}"/>
            <Rectangle x:Name="Yin_Copy4" Height="{DynamicResource SizeOfLines}" Margin="0,0,0,4" Fill="{DynamicResource Yin}"/>
        </StackPanel>
        <StackPanel Grid.Column="2" HorizontalAlignment="Left" Height="200" Margin="5,10,0,0" Grid.Row="2" VerticalAlignment="Top" Width="170" Grid.RowSpan="2">
            <TextBox x:Name="textBox" HorizontalAlignment="Right" Height="20" Margin="{DynamicResource SmallNamesMargin}" TextWrapping="Wrap" Text="13. Splitting Apart ䷼" VerticalAlignment="Top" Width="180" Foreground="White" Background="{x:Null}" BorderBrush="{x:Null}" SelectionBrush="{x:Null}" FontSize="14" TextAlignment="Justify" FontFamily="Segoe UI Semibold"/>
            <TextBox HorizontalAlignment="Left" Height="20" Margin="{DynamicResource SmallNamesMargin}" TextWrapping="Wrap" Text="13. Inner Truth ䷼" VerticalAlignment="Top" Width="175" Foreground="White" Background="{x:Null}" BorderBrush="{x:Null}" SelectionBrush="{x:Null}" FontSize="14" TextAlignment="Justify" FontFamily="Segoe UI Semibold"/>
            <TextBox HorizontalAlignment="Left" Height="20" Margin="{DynamicResource SmallNamesMargin}" TextWrapping="Wrap" Text="13. Inner Truth ䷼" VerticalAlignment="Top" Width="175" Foreground="White" Background="{x:Null}" BorderBrush="{x:Null}" SelectionBrush="{x:Null}" FontSize="14" TextAlignment="Justify" FontFamily="Segoe UI Semibold"/>
            <TextBox HorizontalAlignment="Left" Height="20" Margin="{DynamicResource SmallNamesMargin}" TextWrapping="Wrap" Text="13. Inner Truth ䷼" VerticalAlignment="Top" Width="175" Foreground="White" Background="{x:Null}" BorderBrush="{x:Null}" SelectionBrush="{x:Null}" FontSize="14" TextAlignment="Justify" FontFamily="Segoe UI Semibold"/>
            <TextBox HorizontalAlignment="Left" Height="20" Margin="{DynamicResource SmallNamesMargin}" TextWrapping="Wrap" Text="13. Inner Truth ䷼" VerticalAlignment="Top" Width="175" Foreground="White" Background="{x:Null}" BorderBrush="{x:Null}" SelectionBrush="{x:Null}" FontSize="14" TextAlignment="Justify" FontFamily="Segoe UI Semibold"/>
            <TextBox HorizontalAlignment="Left" Height="20" Margin="{DynamicResource SmallNamesMargin}" TextWrapping="Wrap" Text="13. Inner Truth ䷼" VerticalAlignment="Top" Width="175" Foreground="White" Background="{x:Null}" BorderBrush="{x:Null}" SelectionBrush="{x:Null}" FontSize="14" TextAlignment="Justify" FontFamily="Segoe UI Semibold"/>
        </StackPanel>
        <Label Content="6. Sung / Conflict" Foreground="White" Margin="0,0,0,214" FontWeight="Bold" FontSize="24" Grid.ColumnSpan="3" HorizontalAlignment="Center" RenderTransformOrigin="0.544,0.196" Grid.Row="1" FontFamily="Meiryo"/>
        <ScrollViewer Grid.ColumnSpan="3" Margin="0,45,10,10" Grid.Row="1">
            <StackPanel HorizontalAlignment="Left" Width="660" Height="225" VerticalAlignment="Bottom" RenderTransformOrigin="0.523,0.392">
                <Grid Height="50">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="10*"/>
                        <ColumnDefinition/>
                    </Grid.ColumnDefinitions>
                    <TextBox TextWrapping="Wrap" Text="Above CH'IEN, / THE CREATIVE, HEAVEN &#xD;&#xA;Below K'UN / THE RECEPTIVE " VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Grid.ColumnSpan="2" BorderBrush="{x:Null}" FontSize="14"/>
                    <TextBox TextWrapping="Wrap" Text="☰&#xD;&#xA;☷" VerticalContentAlignment="Center" HorizontalContentAlignment="Right" Grid.Column="1" BorderBrush="{x:Null}" FontSize="14" Margin="0,0,10,0"/>
                </Grid>
                <TextBox Height="30" TextWrapping="Wrap" Text="TextBox" Background="{x:Null}" Foreground="White" BorderBrush="{x:Null}"/>
                <Grid Height="40">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="29*"/>
                        <ColumnDefinition Width="103*"/>
                    </Grid.ColumnDefinitions>
                    <TextBox HorizontalAlignment="Left" Height="40" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="145"/>
                    <TextBox HorizontalAlignment="Left" Height="40" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="515" Grid.Column="1"/>
                </Grid>
                <TextBox HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="660"/>
                <Grid Height="40">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="29*"/>
                        <ColumnDefinition Width="103*"/>
                    </Grid.ColumnDefinitions>
                    <TextBox HorizontalAlignment="Left" Height="40" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="145"/>
                    <TextBox HorizontalAlignment="Left" Height="40" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="515" Grid.Column="1"/>
                </Grid>
                <TextBox Height="30" TextWrapping="Wrap" Text="TextBox" Background="{x:Null}" Foreground="White" BorderBrush="{x:Null}"/>
            </StackPanel>
        </ScrollViewer>
    </Grid>
</Window>
