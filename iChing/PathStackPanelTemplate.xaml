<UserControl x:Class="iChing.PathStackPanelTemplate"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="195" d:DesignWidth="135">
    <StackPanel HorizontalAlignment="Left" Height="195" VerticalAlignment="Top" Width="135" Background="#66FF0000" Orientation="Horizontal">
        <StackPanel Width="50">
            <Button Content="Button" Click="Button_Click"/>
            <Button Content="Button"/>
        </StackPanel>
    </StackPanel>
</UserControl>
